<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sqds.ces.biz.mapper.TPlotMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.sqds.ces.biz.entity.TPlot">
        <id column="id" property="id" />
        <result column="plot_name" property="plotName" />
        <result column="city_did_id" property="cityDidId" />
        <result column="plot_address" property="plotAddress" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, plot_name, city_did_id, plot_address
    </sql>

</mapper>
